<div id="criminal-record-wrapper">

    <div id="menu">
        <div id="header">
            <div class="left">
                <a href="/api/criminal-record/casier" class="logo"><img src="/images/logo.png" class="image" alt="LOGO"
                                                                        draggable="false" /></a><br>
                <h1>Casier Judiciaire</h1></a>

            </div>
            <div class="right" id="bar">
                <ul>
                    <li>
                        <p>Created</p>
                        <p>01-12-2022</p>
                    </li>
                    <li>
                        <p>Version</p>
                        <p>1.0</p>
                    </li>
                    <p class="small-txt">Generated by the state computer</p>
                </ul>
            </div>
        </div>

        <div class="row" id="menu-content">

            <div id="welcome">
                <table class="centered result">
                    <thead>
                    <tr>
                        <th>Prénom</th>
                        <th>Nom</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% data.forEach(d => {%>
                        <tr>
                            <td><%= d.Noms %></td>
                            <td><%= d.Prenom %></td>
                            <td><a href="/api/criminal-record/casier/<%= d.id_criminel %>"><button>ouvrire</button></a></td>
                        </tr>
                    <% }); %>
                    </tbody>
                </table>
                <a href="/api/criminal-record/casier"><button class="btn-large btn-flat waves-effect waves-light col s12">Précedent</button></a>
            </div>

        </div>
    </div>
</div>
<script src="/js/jquery.js" type="text/javascript"></script>
